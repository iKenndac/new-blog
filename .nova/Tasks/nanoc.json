{
  "actions" : {
    "build" : {
      "enabled" : true,
      "script" : "#!\/bin\/sh\nbundle exec nanoc\n"
    },
    "clean" : {
      "enabled" : true,
      "script" : "#!\/bin\/sh\nrm -r .\/output\n"
    },
    "run" : {
      "enabled" : true,
      "script" : "#!\/bin\/sh\nbundle exec nanoc; bundle exec nanoc view\n"
    }
  },
  "openLogOnRun" : "fail"
}
